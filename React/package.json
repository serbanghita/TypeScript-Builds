  "license": "MIT",
  "dependencies": {
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "browserify": "^14.3.0",
    "gulp": "^3.9.1",
    "tsc": "^1.20150623.0",
    "tsify": "^3.0.1",
    "tslint": "^5.2.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.2",
    "vinyl-source-stream": "^1.1.0"
  }
}
