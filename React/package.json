{
  "name": "react-typescript-example",
  "version": "1.0.0",
  "description": "React with TypeScript example.",
  "scripts": {
    "tsc": "$(npm bin)/tsc",
    "build": "npm run tsc && npm run browserify",
    "browserify": "$(npm bin)/browserify dist/js/index.js -o dist/js/bundle.browserify.js",
    "serve": "$(npm bin)/browser-sync start --s dist --f dist --port 3000 --reload-debounce 200 --no-ui"
  },
